<AnchorPane prefHeight="254.0" prefWidth="482.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ProductFormController">
   <children>
      <GridPane layoutX="36.0" layoutY="21.0" prefHeight="137.0" prefWidth="370.0" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="21.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="128.0" minWidth="10.0" prefWidth="128.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="107.0" minWidth="10.0" prefWidth="96.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Codigo" />
            <Label text="Descrição" GridPane.rowIndex="1" />
            <Label text="Tipo" GridPane.rowIndex="2" />
            <Label text="Descrição estrangeira" GridPane.rowIndex="3" />
            <TextField fx:id="txtCod" GridPane.columnIndex="1" />
            <TextField fx:id="txtDescr" GridPane.columnIndex="1" GridPane.rowIndex="1" />
