            <TextField fx:id="txtDescest" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="labelErrorName" textFill="#e11414" GridPane.columnIndex="2" />
         </children>
      </GridPane>
      <HBox layoutX="157.0" layoutY="158.0" prefHeight="25.0" prefWidth="144.0" spacing="5.0">
         <children>
            <Button fx:id="btSalvar" mnemonicParsing="false" onAction="#onBtSalvarAction" text="Salvar" />
            <Button fx:id="btCancelar" mnemonicParsing="false" onAction="#onBtCancelarAction" text="Cancelar" />
         </children>
      </HBox>
   </children>
</AnchorPane>
